{
  "routes": [
    {
      "src": "/",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "X-Content-Type-Options": "nosniff",
        "Feature-Policy": "*",
        "Referrer-Policy": "origin"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.+)",
      "dest": "/",
      "status": 301
    }
  ]
}
